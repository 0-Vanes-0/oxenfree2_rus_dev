{
    "bundle": "loc_packages_assets_",
    "scene": "A2V3.HUMBAR",
    "entries": [
        {
            "tag": "A2V3.HUMBAR_CHARLIE_0000",
            "en": "Olivia, this is crazy!",
            "ru_native": "Оливия, это безумие!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, це божевілля!"
        },
        {
            "tag": "A2V3.HUMBAR_CHARLIE_0001",
            "en": "Quit it, Olivia, this crap hurts!",
            "ru_native": "Оливия, твою мать! Больно!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, твою ж!.. Боляче!"
        },
        {
            "tag": "A2V3.HUMBAR_CHARLIE_0002",
            "en": "Olivia, stop, you have no idea what that’s going to do!",
            "ru_native": "Оливия, стой! Ты не понимаешь, что делаешь!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, стій, ти не розумієш, що робиш!"
        },
        {
            "tag": "A2V3.HUMBAR_CHARLIE_0003",
            "en": "Stay away from her!",
            "ru_native": "Не подходи к ней!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не підходь до неї!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0000",
            "en": "Riley! Get Olivia’s radio before she tunes into the triangle!",
            "ru_native": "Райли! Хватай радио, пока Оливия на настроилась на треугольник!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі! Хапай радіо, доки Олівія не налаштувалася на трикутник!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0001",
            "en": "Olivia, stop! You don’t know what you’re doing!",
            "ru_native": "Оливия, стой! Ты не понимаешь, что делаешь!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, зупинися! Ти не знаєш, що ти робиш!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0002",
            "en": "Riley, stop Olivia! We’re running out of time!",
            "ru_native": "Райли, останови Оливию! У нас мало времени!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі, зупини Олівію! У нас мало часу!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0003",
            "en": "The radio’s right there, just take it!",
            "ru_native": "Радио прямо тут, хватай!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Радіо ось тут, хапай його!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0004",
            "en": "She’s getting closer, Riley, hurry!",
            "ru_native": "Райли, она все ближе, скорее!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вона наближається, Райлі, швидше!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0005",
            "en": "You can’t let her open the portal!",
            "ru_native": "Нельзя дать ей открыть портал!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не можна дати їй відкрити портал!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0006",
            "en": "Riley, she’s more than halfway done!",
            "ru_native": "Райли, половина уже готова!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі, половина вже готова!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0007",
            "en": "The portal’s getting drawn, it’s now or never!",
            "ru_native": "Портал проявляется... Сейчас или никогда!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Портал з’являється… Зараз або ніколи!"
        },
        {
            "tag": "A2V3.HUMBAR_JACOB_0008",
            "en": "Don’t let her finish the portal, Riley!",
            "ru_native": "Не дай ей закончить портал, Райли!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не дай їй закінчити портал, Райлі!"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0000",
            "en": "Just—just a little more…",
            "ru_native": "Еще... Еще чуть-чуть...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ще… ще трохи…"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0001",
            "en": "God damnit, I just need…",
            "ru_native": "Черт, нужно только...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Дідько, треба лише…"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0002",
            "en": "Ergh, what’s the signal?",
            "ru_native": "Да какой тут сигнал?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Та який тут сигнал?"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0003",
            "en": "Almost… there…",
            "ru_native": "Почти... готово...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Майже… вдалося…"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0004",
            "en": "Stay back!",
            "ru_native": "Назад!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Тримайся подалі!"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0005",
            "en": "I can do this without any of you!",
            "ru_native": "Я могу все сделать сама!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я можу зробити все сама!"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0006",
            "en": "I hate you! Keep away from me!",
            "ru_native": "Ненавижу вас! Не подходите!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ненавиджу вас! Не підходьте!"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0007",
            "en": "This will work, just—just watch!",
            "ru_native": "Он сработает, вот увидите!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Це спрацює, от побачите!"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0008",
            "en": "I’m almost done, just let me do this!",
            "ru_native": "Почти готово, дайте мне закончить!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Майже готово, дайте мені закінчити!"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0009",
            "en": "You can’t follow me!",
            "ru_native": "Отошли от меня!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Відійдіть від мене!"
        },
        {
            "tag": "A2V3.HUMBAR_OLIVIA_0010",
            "en": "Get the Hell away!",
            "ru_native": "Отвалите все!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Відваліть усі!"
        },
        {
            "tag": "A2V3.HUMBAR_VIOLET_0000",
            "en": "Olivia, stop!",
            "ru_native": "Оливия, стой!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, стій!"
        },
        {
            "tag": "A2V3.HUMBAR_VIOLET_0001",
            "en": "Olivia, what are you doing?!",
            "ru_native": "Оливия, что ты делаешь?!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, що ти робиш?!"
        },
        {
            "tag": "A2V3.HUMBAR_VIOLET_0002",
            "en": "Oh my God, Olivia, what are we doing here?!",
            "ru_native": "О боже, Оливия... Что мы здесь делаем?!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Боже, Олівіє, що ми тут робимо?!"
        },
        {
            "tag": "A2V3.HUMBAR_VIOLET_0003",
            "en": "Olivia, don’t finish that!",
            "ru_native": "Оливия, не соединяй его!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, не закінчуй його!"
        }
    ]
}