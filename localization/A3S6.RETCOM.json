{
    "bundle": "loc_packages_assets_",
    "scene": "A3S6.RETCOM",
    "entries": [
        {
            "tag": "A3S6.RETCOM_CHOICE_0000",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A3S6.RETCOM_CHOICE_0000_OPTION_0000",
            "en": "He’s like me.",
            "ru_native": "Он как я.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Він як я."
        },
        {
            "tag": "A3S6.RETCOM_CHOICE_0000_OPTION_0001",
            "en": "Nothing to be sorry for.",
            "ru_native": "Не за что извиняться.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вибачатися немає за що."
        },
        {
            "tag": "A3S6.RETCOM_CHOICE_0000_OPTION_0002",
            "en": "He was a neat kid.",
            "ru_native": "Он был милым мальчиком.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Він був милим хлопчиком."
        },
        {
            "tag": "A3S6.RETCOM_CHOICE_0001",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A3S6.RETCOM_CHOICE_0001_OPTION_0001",
            "en": "Alex?",
            "ru_native": "Алекс?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Алекс?"
        },
        {
            "tag": "A3S6.RETCOM_CHOICE_0001_OPTION_0002",
            "en": "What do I do now?",
            "ru_native": "Что мне делать?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Що ж тепер робити?"
        },
        {
            "tag": "A3S6.RETCOM_JACOB_0000",
            "en": "Riley, I’m—I’m sorry about… Rex.",
            "ru_native": "Райли, Я... прости за Рекса.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_JACOB_0001",
            "en": "I didn’t…",
            "ru_native": "Я не...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_JACOB_0002",
            "en": "I didn’t know he would be so…",
            "ru_native": "Я не знал, что он будет таким...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_JACOB_0003",
            "en": "I don’t know.",
            "ru_native": "Не знаю.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_JACOB_0004",
            "en": "I’m just sorry.",
            "ru_native": "Просто прости.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_JACOB_0005",
            "en": "Riley, that’s—",
            "ru_native": "Райли, это...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0000",
            "en": "He… ended up just like me.",
            "ru_native": "Он... стал таким же, как я.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0001",
            "en": "After everything that happens, after all those years… he ends up just like me.",
            "ru_native": "После всего случившегося, после всех этих лет... он стал таким, как я.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0002",
            "en": "Lost. Bitter at being born.",
            "ru_native": "Потерянным. Жалеющим, что родился.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0003",
            "en": "There’s nothing to be sorry for, Jacob. Things happen.",
            "ru_native": "Тебе не за что извиняться, Джейкоб. Это жизнь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0004",
            "en": "People are born. They grow old, they change, they die. Rex… just got old.",
            "ru_native": "Люди рождаются. Растут, меняются, умирают. Рекс... просто вырос.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0005",
            "en": "Being my son couldn’t have helped things—",
            "ru_native": "То, что он мой сын, ничем бы не помогло...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0006",
            "en": "He was a neat little kid, he really was. I wish you could’ve met him… then.",
            "ru_native": "Он был милым мальчиком, правда. Хотела бы я, чтобы ты с ним познакомился... тогда.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0007",
            "en": "Instead of, you know, after.",
            "ru_native": "А не потом.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0008",
            "en": "Being my son just caught up with him.",
            "ru_native": "Просто его настигла моя судьба.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0009",
            "en": "Rex… after everything that happens… you just end up like me.",
            "ru_native": "Рекс... после всего, что случилось... ты стал таким же, как я.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0010",
            "en": "I’m so sorry.",
            "ru_native": "Мне так жаль.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0011",
            "en": "Alex? Where are you? I’m back at the Comm Tower!",
            "ru_native": "Алекс? Где ты? Я в башне связи!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Алекс? Ти де? Я в радіовежі!"
        },
        {
            "tag": "A3S6.RETCOM_RILEY_0012",
            "en": "Ah, this again? Crap. What am I supposed to do now?",
            "ru_native": "Что, опять? Черт. И что мне тебе делать?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Що, знову? Бляха. Що мені тепер робити?"
        }
    ]
}
