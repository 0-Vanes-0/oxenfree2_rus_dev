{
    "bundle": "loc_packages_assets_",
    "scene": "A1LH.COMROU",
    "entries": [
        {
            "tag": "A1LH.COMROU_CHOICE_0000",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A1LH.COMROU_CHOICE_0000_OPTION_0000",
            "en": "I like a challenge.",
            "ru_native": "Я не боюсь трудностей.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Мені подобаються труднощі."
        },
        {
            "tag": "A1LH.COMROU_CHOICE_0000_OPTION_0001",
            "en": "Live a little.",
            "ru_native": "Расслабься.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Насолодися життям трохи."
        },
        {
            "tag": "A1LH.COMROU_CHOICE_0000_OPTION_0002",
            "en": "Maybe you’re right...",
            "ru_native": "Думаю, ты прав...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Можливо, ти маєш рацію…"
        },
        {
            "tag": "A1LH.COMROU_JACOB_0000",
            "en": "Oh wait, we can—th-the walking path’s here.",
            "ru_native": "Стой, тут можно... Здесь тропинка.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Чекай, ми можемо… тут прохід."
        },
        {
            "tag": "A1LH.COMROU_JACOB_0001",
            "en": "It’s a lot easier.",
            "ru_native": "Так проще.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Це набагато простіше."
        },
        {
            "tag": "A1LH.COMROU_JACOB_0002",
            "en": "So does not falling off slippery rocks into the ocean.",
            "ru_native": "Если не падать в океан, тоже много чего сохранишь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Як і спроби не зісковзнути зі слизького каміння в океан."
        },
        {
            "tag": "A1LH.COMROU_JACOB_0003",
            "en": "But hey, you do you.",
            "ru_native": "Хотя решай сама.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Гей, тобі вирішувати."
        },
        {
            "tag": "A1LH.COMROU_JACOB_0004",
            "en": "I-I’d rather live uh—ya know—a lot.",
            "ru_native": "Я бы расслабился, но не здесь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Хотілося б насолоджуватися життям довго."
        },
        {
            "tag": "A1LH.COMROU_JACOB_0005",
            "en": "Hence avoiding slippery rock climbing.",
            "ru_native": "Не карабкаясь по скользким скалам.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Тому слід уникати слизького каміння."
        },
        {
            "tag": "A1LH.COMROU_JACOB_0006",
            "en": "No, hey, you do you.",
            "ru_native": "Нет-нет, решай сама.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ні, тобі вирішувати."
        },
        {
            "tag": "A1LH.COMROU_RILEY_0000",
            "en": "What if I like a challenge?",
            "ru_native": "Я трудностей не боюсь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Що як мені подобаються труднощі?"
        },
        {
            "tag": "A1LH.COMROU_RILEY_0001",
            "en": "Keeps you young, ya know?",
            "ru_native": "Они сохраняют молодость.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Тримають у тонусі, розумієш?"
        },
        {
            "tag": "A1LH.COMROU_RILEY_0002",
            "en": "Aw, c’mon, live a little.",
            "ru_native": "Да ладно, расслабься.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Годі тобі, насолодися життям трохи."
        },
        {
            "tag": "A1LH.COMROU_RILEY_0003",
            "en": "Eh, maybe you’re right…",
            "ru_native": "Да, думаю, ты прав...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Можливо, ти маєш рацію…"
        }
    ]
}