        {
            "tag": "A2G2.ARRIVE_JACOB_0052",
            "en": "Because I am disappointed.",
            "ru_native": "Потому что я разочарован.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Адже я розчарований."
