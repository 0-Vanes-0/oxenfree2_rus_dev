    "entries": [
        {
            "tag": "A2C4.THEGAP_CHOICE_0000",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A2C4.THEGAP_CHOICE_0000_OPTION_0000",
            "en": "Let’s look.",
            "ru_native": "Осмотримся.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Подивимося."
        },
        {
            "tag": "A2C4.THEGAP_CHOICE_0000_OPTION_0001",
            "en": "You jump as a test.",
            "ru_native": "Прыгни и проверь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Стрибай, перевір."
        },
        {
            "tag": "A2C4.THEGAP_JACOB_0000",
            "en": "Jesus, someone should put uh—cones here or something, right?",
            "ru_native": "Боже, тут надо ставить какие-нибудь... ограждения.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Господи, поставили б якісь… огородження."
        },
        {
            "tag": "A2C4.THEGAP_JACOB_0001",
            "en": "Maybe there’s another way around?",
            "ru_native": "Может, есть обходной путь?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Може є інша дорога?"
        },
        {
            "tag": "A2C4.THEGAP_JACOB_0002",
            "en": "We need to get to the other side.",
            "ru_native": "Нам нужно на ту сторону.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Нам треба на ту сторону."
        },
        {
            "tag": "A2C4.THEGAP_JACOB_0003",
            "en": "Ha. No. I like my bones where they are.",
            "ru_native": "Ну уж нет. Мне мои кости дороги.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ха-ха. Я б хотів, щоб усі мої кісточки залишалися на своїх місцях."
        },
        {
            "tag": "A2C4.THEGAP_JACOB_0004",
            "en": "I’m sure there’s another route.",
            "ru_native": "Уверен, есть обходной путь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Впевнений, є інший шлях."
        },
        {
            "tag": "A2C4.THEGAP_RILEY_0000",
            "en": "Definitely not jumping that one.",
            "ru_native": "Здесь я точно не перелечу.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Тут я точно не стрибатиму."
        },
        {
            "tag": "A2C4.THEGAP_RILEY_0001",
            "en": "Yeah, let’s look around—find a detour.",
            "ru_native": "Да, давай поищем обходной путь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Так, пошукаймо обхідний шлях."
        },
        {
            "tag": "A2C4.THEGAP_RILEY_0002",
            "en": "Eh, maybe it’s not as far as it looks? You go first.",
            "ru_native": "Может, тут не так уж далеко? Попробуй.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ех, може тут насправді не так уже й далеко? Спробуй-но."
        }
    ]
