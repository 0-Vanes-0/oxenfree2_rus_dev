{
    "bundle": "loc_packages_assets_",
    "scene": "A1S3.EVIRIT",
    "entries": [
        {
            "tag": "A1S3.EVIRIT_JACOB_0000",
            "en": "Oh. That?",
            "ru_native": "А. Это?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "А. Це?"
        },
        {
            "tag": "A1S3.EVIRIT_JACOB_0001",
            "en": "We have some… off-color people living this far out from proper civilization.",
            "ru_native": "У нас тут какие-то... странные люди, живущие вдали от цивилизации.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "У нас тут якісь… дивні люди, що живуть поза цивілізацією."
        },
        {
            "tag": "A1S3.EVIRIT_JACOB_0002",
            "en": "There’s a group in town that performs, uh, nature rituals.",
            "ru_native": "В городе есть группа, которая проводит, хм, природные ритуалы.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "У місті є група людей, яка виконує, гм, природні ритуали."
        },
        {
            "tag": "A1S3.EVIRIT_JACOB_0003",
            "en": "You’ll see evidence of it dotted around.",
            "ru_native": "Тут повсюду их следы.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Тут всюди їхні сліди."
        },
        {
            "tag": "A1S3.EVIRIT_JACOB_0004",
            "en": "This could be them.",
            "ru_native": "Может, это они.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Можливо, це вони."
        },
        {
            "tag": "A1S3.EVIRIT_RILEY_0000",
            "en": "What’s… this?",
            "ru_native": "Что... это?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Що… це?"
        },
        {
            "tag": "A1S3.EVIRIT_RILEY_0001",
            "en": "[to herself] Oh, this is… weird.",
            "ru_native": "Ох, это... странно.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ох, це… дивно."
        },
        {
            "tag": "A1S3.EVIRIT_RILEY_0002",
            "en": "[to herself] It’s like someone made a little… forest art project or something?",
            "ru_native": "Похоже на какой-то... лесной арт-объект или типа того?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Схоже на якийсь… лісовий арт-об’єкт чи щось таке?"
        },
        {
            "tag": "A1S3.EVIRIT_RILEY_0003",
            "en": "[to herself] Kinda creepy, to be honest…",
            "ru_native": "Жутковато, если честно...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Трохи моторошно, якщо чесно…"
        }
    ]
}