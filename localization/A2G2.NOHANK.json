{
    "bundle": "loc_packages_assets_",
    "scene": "A2G2.NOHANK",
    "entries": [
        {
            "tag": "A2G2.NOHANK_CHOICE_0000",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A2G2.NOHANK_CHOICE_0000_OPTION_0000",
            "en": "Nope.",
            "ru_native": "Не-а.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Та ні."
        },
        {
            "tag": "A2G2.NOHANK_CHOICE_0000_OPTION_0001",
            "en": "This is more important.",
            "ru_native": "Это куда важнее.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Це набагато важливіше."
        },
        {
            "tag": "A2G2.NOHANK_CHOICE_0000_OPTION_0002",
            "en": "Maybe later.",
            "ru_native": "Может, позже.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Може, пізніше."
        },
        {
            "tag": "A2G2.NOHANK_JACOB_0000",
            "en": "So we’re not helping out Hank?",
            "ru_native": "Значит, мы не помогаем Хэнку?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Значить, ми не допомагаємо Хенку?"
        },
        {
            "tag": "A2G2.NOHANK_JACOB_0001",
            "en": "Cool with me, just checking.",
            "ru_native": "Я не против, просто уточняю.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я не проти, просто уточнюю."
        },
        {
            "tag": "A2G2.NOHANK_JACOB_0002",
            "en": "And this is an okay.",
            "ru_native": "А это – «да».",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "А це — «гаразд»."
        },
        {
            "tag": "A2G2.NOHANK_JACOB_0003",
            "en": "Oh without half a doubt.",
            "ru_native": "Не сомневаюсь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Буз сумніву."
        },
        {
            "tag": "A2G2.NOHANK_JACOB_0004",
            "en": "I’m getting “no” energy off you, so, going with that.",
            "ru_native": "Чувствую, ты хочешь сказать «нет».",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Відчуваю, що ти хочеш сказати «ні». Тож погоджуюся."
        },
        {
            "tag": "A2G2.NOHANK_JACOB_0005",
            "en": "Carry on.",
            "ru_native": "Продолжай.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Продовжуй."
        },
        {
            "tag": "A2G2.NOHANK_JACOB_0006",
            "en": "As far as we know, at least.",
            "ru_native": "Насколько мы знаем.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Принаймні, наскільки нам відомо."
        },
        {
            "tag": "A2G2.NOHANK_JACOB_0007",
            "en": "But point taken.",
            "ru_native": "Но я понял.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Але зрозуміло."
        },
        {
            "tag": "A2G2.NOHANK_RILEY_0000",
            "en": "That’s a no.",
            "ru_native": "Это – «нет».",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Це — «ні»."
        },
        {
            "tag": "A2G2.NOHANK_RILEY_0001",
            "en": "This is way more important.",
            "ru_native": "Это гораздо важнее.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Це набагато важливіше."
        },
        {
            "tag": "A2G2.NOHANK_RILEY_0002",
            "en": "Why not? Those tear things aren't going anywhere.",
            "ru_native": "Почему бы и нет? Разрывы никуда не денутся.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Чому ні? Ці розриви нікуди не подінуться."
        }
    ]
}