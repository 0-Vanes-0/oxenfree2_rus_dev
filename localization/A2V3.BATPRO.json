{
    "bundle": "loc_packages_assets_",
    "scene": "A2V3.BATPRO",
    "entries": [
        {
            "tag": "A2V3.BATPRO_CHARLIE_0001",
            "en": "Violet, just don’t move.",
            "ru_native": "Вайолет, не двигайся.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вайолет, не ворушися."
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0002",
            "en": "I see the radio! It’s over -- crap, it’s --",
            "ru_native": "Вижу радио! Все кончено... вот дерьмо...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я бачу радіо! Це кінець… от лайно…"
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0003",
            "en": "You’re not getting past me.",
            "ru_native": "Ты не пройдешь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ти не пройдеш."
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0004",
            "en": "No chance.",
            "ru_native": "И не пытайся.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ні за що."
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0005",
            "en": "Not gonna happen.",
            "ru_native": "Ничего не выйдет.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не бувати цьому."
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0006",
            "en": "No way, lady.",
            "ru_native": "Стоять, дамочка.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Стояти, леді."
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0007",
            "en": "I’m sorry, Olivia, but Riley’s been nicer to me than you have in eight months.",
            "ru_native": "Прости, Оливия, но Райли была добрее ко мне, чем ты за эти 8 месяцев.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вибач, Олівіє, але Райлі була добрішою до мене, ніж ти за ці вісім місяців."
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0008",
            "en": "And -- you’ve -- I don’t know, ever since your parents --",
            "ru_native": "И ты... Не знаю, с тех пор, как твои предки...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "А… ти… Не знаю, з того часу, як твої батьки…"
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0009",
            "en": "No!",
            "ru_native": "Нет!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ні!"
        },
        {
            "tag": "A2V3.BATPRO_CHARLIE_0010",
            "en": "No, Olivia. You need to stop.",
            "ru_native": "Нет, Оливия. Остановись.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ні, Олівіє. Зупинися."
        },
        {
            "tag": "A2V3.BATPRO_JACOB_0000",
            "en": "Riley! we need to stop her!",
            "ru_native": "Райли! Нужно ее остановить!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі! Нам треба її зупинити!"
        },
        {
            "tag": "A2V3.BATPRO_JACOB_0001",
            "en": "Violet, back away from it! It will only hurt you and everyone else here.",
            "ru_native": "Вайолет, отойди! Ты только навредишь себе и всем остальным.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вайолет, відійди! Ти лише нашкодиш собі та іншим."
        },
        {
            "tag": "A2V3.BATPRO_JACOB_0002",
            "en": "God! Ergh.",
            "ru_native": "Боже!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Боже!"
        },
        {
            "tag": "A2V3.BATPRO_JACOB_0003",
            "en": "Violet, don’t!",
            "ru_native": "Вайолет, нет!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вайолет, ні!"
        },
        {
            "tag": "A2V3.BATPRO_JACOB_0004",
            "en": "Riley, c’mon, get it!",
            "ru_native": "Райли, за ним!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі, давай, хапай його!"
        },
        {
            "tag": "A2V3.BATPRO_JACOB_0005",
            "en": "Okay, this sucks. Can we stop this, please?",
            "ru_native": "Так, дело труба. Может, прекратим все это?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Так, це фігня якась. Може, припинимо все це?"
        },
        {
            "tag": "A2V3.BATPRO_JACOB_0006",
            "en": "Riley!",
            "ru_native": "Райли!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі!"
        },
        {
            "tag": "A2V3.BATPRO_JACOB_0007",
            "en": "Let her get it!",
            "ru_native": "Пусть забирает!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Хай забирає!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0000",
            "en": "STAY AWAY FROM ME!",
            "ru_native": "НЕ ПОДХОДИТЕ!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "НЕ ПІДХОДЬТЕ!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0001",
            "en": "Where’s the -- there! At your feet! Violet!",
            "ru_native": "Где... вон! У твоей ноги! Вайолет!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Де… ось! У тебе під ногами! Вайолет!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0002",
            "en": "Keep them away from me!",
            "ru_native": "Не дайте им подойти!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не дайте їм наблизитися!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0003",
            "en": "Get our of the way Violet!",
            "ru_native": "С дороги, Вайолет!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не заважай, Вайолет!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0004",
            "en": "It’s right there!",
            "ru_native": "Вот оно!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ось воно!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0005",
            "en": "MOVE!",
            "ru_native": "ОТОЙДИ!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "ВІДІЙДИ!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0006",
            "en": "Violet!",
            "ru_native": "Вайолет!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вайолет!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0007",
            "en": "Whoa!",
            "ru_native": "Ого!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ого!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0008",
            "en": "If she gets that radio, it’s all over!",
            "ru_native": "Если она заполучит радио, нам крышка!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Якщо вона дістане радіо, то нам гаплик!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0009",
            "en": "Yes! Yes -- I’m -- I’m ready, just -- just toss it.",
            "ru_native": "Да! Да... Я готова. Бросай.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Так! Так… Я готова… Кидай."
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0010",
            "en": "Violet!",
            "ru_native": "Вайолет!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вайолет!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0011",
            "en": "Ah, there you --",
            "ru_native": "Вот ты...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ось і ти…"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0012",
            "en": "Yes, stop her!",
            "ru_native": "Да, останови ее!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Так, зупини її!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0013",
            "en": "Stay -- stay in her way -- just a sec --",
            "ru_native": "Держи... Держи ее... Еще секунду...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не… пропускай її… ще трохи…"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0014",
            "en": "Just -- just stay there.",
            "ru_native": "Стой... стой там!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Стій… стій там."
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0015",
            "en": "Don’t move!",
            "ru_native": "Не двигайся!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не рухайся!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0016",
            "en": "Charlie! Get -- can you get out of my way, please.",
            "ru_native": "Чарли! Убир... Отойди с дороги, пожалуйста.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Чарлі! Забир… Відійди з дороги, будь ласка."
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0017",
            "en": "Get out of the way!",
            "ru_native": "Убирайся с дороги!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Відійди з дороги!"
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0018",
            "en": "I’m gonna chew the tongue out of your God damned head if you don’t move.",
            "ru_native": "Я вырву язык из твоей тупой башки, если не свалишь по-хорошему.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я вирву язик із твоєї тупої макітри, якщо ти не звалиш."
        },
        {
            "tag": "A2V3.BATPRO_OLIVIA_0019",
            "en": "I'm going to dial in",
            "ru_native": "Я набираю...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я набираю…"
        },
        {
            "tag": "A2V3.BATPRO_RILEY_0000",
            "en": "Ah!",
            "ru_native": "А!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "А!"
        },
        {
            "tag": "A2V3.BATPRO_RILEY_0001",
            "en": "No! Don’t pick it up -- don’t touch it -- it’s -- you don’t know what it can do!",
            "ru_native": "Нет! Не поднимай... не трогай его! Ты не знаешь, что оно может сделать!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ні! Не бери… не чіпай його! Ти не знаєш, що воно може зробити!"
        },
        {
            "tag": "A2V3.BATPRO_RILEY_0002",
            "en": "Hi. I’ll take that.",
            "ru_native": "Привет. Я заберу.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Привіт. Я заберу."
        },
        {
            "tag": "A2V3.BATPRO_RILEY_0003",
            "en": "There! Ha!",
            "ru_native": "Есть! Ха!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Є! Ха!"
        },
        {
            "tag": "A2V3.BATPRO_RILEY_0004",
            "en": "Yeah, I’m going for it!",
            "ru_native": "Да, лечу!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Так, лечу!"
        },
        {
            "tag": "A2V3.BATPRO_RILEY_0005",
            "en": "Give me that!",
            "ru_native": "Отдай!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Віддай!"
        },
        {
            "tag": "A2V3.BATPRO_VIOLET_0000",
            "en": "Olivia are you sure?? ",
            "ru_native": "Оливия, ты уверена?! ",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, ти впевнена? "
        },
        {
            "tag": "A2V3.BATPRO_VIOLET_0001",
            "en": "Stop Olivia!",
            "ru_native": "Оливия, перестань!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, припини!"
        },
        {
            "tag": "A2V3.BATPRO_VIOLET_0002",
            "en": "You need to stop this Olivia",
            "ru_native": "Оливия, нужно это прекратить.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, припини це."
        },
        {
            "tag": "A2V3.BATPRO_VIOLET_0003",
            "en": "Olivia, don’t do this!",
            "ru_native": "Оливия, не делай этого!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє, не роби цього!"
        },
        {
            "tag": "A2V3.BATPRO_VIOLET_0004",
            "en": "Olivia? Are you --",
            "ru_native": "Оливия? Ты...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Олівіє? Ти…"
        },
        {
            "tag": "A2V3.BATPRO_VIOLET_0005",
            "en": "Don’t hurt me.",
            "ru_native": "Не трогай меня.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не чіпай мене."
        },
        {
            "tag": "A2V3.BATPRO_VIOLET_0006",
            "en": "I don’t -- I don’t know you!",
            "ru_native": "Я не... Я тебя не знаю!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я не… Я тебе не знаю!"
        }
    ]
}
