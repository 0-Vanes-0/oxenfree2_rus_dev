{
    "bundle": "loc_packages_assets_",
    "scene": "A2C4.THEGHO",
    "entries": [
        {
            "tag": "A2C4.THEGHO_CHOICE_0000",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A2C4.THEGHO_CHOICE_0000_OPTION_0000",
            "en": "Show yourself!",
            "ru_native": "Покажи себя!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Покажися!"
        },
        {
            "tag": "A2C4.THEGHO_CHOICE_0000_OPTION_0001",
            "en": "Who’s there?",
            "ru_native": "Кто здесь?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Хто тут?"
        },
        {
            "tag": "A2C4.THEGHO_CHOICE_0000_OPTION_0002",
            "en": "Stay still.",
            "ru_native": "Замри.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Замри."
        },
        {
            "tag": "A2C4.THEGHO_CHOICE_0001",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A2C4.THEGHO_CHOICE_0001_OPTION_0000",
            "en": "Maybe a shadow…",
            "ru_native": "Может, это тень.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Може, то тінь?"
        },
        {
            "tag": "A2C4.THEGHO_CHOICE_0001_OPTION_0001",
            "en": "We’re not alone.",
            "ru_native": "Мы не одни.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ми не самі."
        },
        {
            "tag": "A2C4.THEGHO_CHOICE_0001_OPTION_0002",
            "en": "Don’t think about it.",
            "ru_native": "Выбрось из головы.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не думай про це."
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0000",
            "en": "Oh, wait wait wait—you see that?",
            "ru_native": "Стой, стой! Ты это видишь?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Чекай-чекай… Ти це бачиш?"
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0001",
            "en": "Hello?",
            "ru_native": "Ау?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Агов?"
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0002",
            "en": "Hey! Is—is someone there?",
            "ru_native": "Эй! Есть тут кто?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Гей! Є там хтось?"
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0003",
            "en": "Well, uh…",
            "ru_native": "М-да...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ну…"
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0004",
            "en": "That was weird.",
            "ru_native": "Странно.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Дивно це."
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0005",
            "en": "At least it wasn’t bear-shaped.",
            "ru_native": "Хорошо, что не тень медведя.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Принаймні, вона була не у формі ведмедя."
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0006",
            "en": "Yeah. Maybe…",
            "ru_native": "Да. Возможно...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Та-ак. Може…"
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0007",
            "en": "Could’ve been a trick of the eye… you, uh, never know.",
            "ru_native": "Наверное, просто обман зрения. Никогда не знаешь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Можливо, якась оптична ілюзія… Хто її зна."
        },
        {
            "tag": "A2C4.THEGHO_JACOB_0008",
            "en": "Deleting from my brain cartridges…",
            "ru_native": "Стираю файлы из памяти...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Триває видалення файлів із пам’яті…"
        },
        {
            "tag": "A2C4.THEGHO_RILEY_0000",
            "en": "Hey, whoever you are! Show yourself! Now!",
            "ru_native": "Эй! Кто бы ты ни был, покажи себя!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Слухай там, хто б ти не був! Покажися! Негайно!"
        },
        {
            "tag": "A2C4.THEGHO_RILEY_0001",
            "en": "Who’s—who’s out there?",
            "ru_native": "Кто... Кто здесь?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Хто… Хто там?"
        },
        {
            "tag": "A2C4.THEGHO_RILEY_0002",
            "en": "Jacob, shhh. Stay still.",
            "ru_native": "Джейкоб, тише. Замри.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Джейкобе, тихо. Замри."
        },
        {
            "tag": "A2C4.THEGHO_RILEY_0003",
            "en": "Maybe… it was just a shadow.",
            "ru_native": "Может, это просто тень.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Може… то була просто тінь."
        },
        {
            "tag": "A2C4.THEGHO_RILEY_0004",
            "en": "I… don’t think we’re alone...",
            "ru_native": "Кажется, мы здесь не одни.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Мені здається… що ми не самі…"
        },
        {
            "tag": "A2C4.THEGHO_RILEY_0005",
            "en": "Just, uh, don’t—don’t think about it.",
            "ru_native": "Знаешь... Выброси это из головы.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Просто… не думай про це."
        }
    ]
}
