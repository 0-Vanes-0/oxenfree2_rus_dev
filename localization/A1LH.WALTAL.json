{
    "bundle": "loc_packages_assets_",
    "scene": "A1LH.WALTAL",
    "entries": [
        {
            "tag": "A1LH.WALTAL_CHARLIE_0000",
            "en": "We can’t get scared now.",
            "ru_native": "Не время очковать.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0000",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0000_OPTION_0000",
            "en": "What did you do?",
            "ru_native": "Что вы сделали?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Що ви зробили?"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0000_OPTION_0001",
            "en": "Who is this?",
            "ru_native": "Кто это?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Хто це?"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0001",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0001_OPTION_0000",
            "en": "Nice talking to you, too.",
            "ru_native": "Приятно пообщаться.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Мені теж приємно розмовляти з вами."
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0001_OPTION_0001",
            "en": "How’d they know?",
            "ru_native": "Как они узнали?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Звідки вони знають?"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0001_OPTION_0002",
            "en": "That didn’t sound human.",
            "ru_native": "Это не человек.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не було схоже на людину."
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0002",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0002_OPTION_0000",
            "en": "Yeah!",
            "ru_native": "Да!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Так!"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0002_OPTION_0001",
            "en": "Those kids, maybe?",
            "ru_native": "Наверно, те дети?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Може, ті діти?"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0003",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0003_OPTION_0000",
            "en": "Wait! Jacob!",
            "ru_native": "Стой! Джейкоб!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Зажди! Джейкобе!"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0003_OPTION_0001",
            "en": "We don’t know them!",
            "ru_native": "Мы их не знаем!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ми їх не знаємо!"
        },
        {
            "tag": "A1LH.WALTAL_CHOICE_0003_OPTION_0002",
            "en": "Don’t run off!",
            "ru_native": "Не убегай!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не тікай!"
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0000",
            "en": "Weird. Sounded like kids.",
            "ru_native": "Странно. Дети какие-то.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0001",
            "en": "Uh… weirder.",
            "ru_native": "Еще страннее...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0002",
            "en": "That one didn’t sound like kids.",
            "ru_native": "Это уже не похоже на детей.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0003",
            "en": "That one…I don’t know what that sounded like.",
            "ru_native": "Даже не знаю, на что это похоже.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0004",
            "en": "Beats me. It’s—weird. Yeah. That’s really weird.",
            "ru_native": "Фиг его знает. Странно это. Очень странно.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0005",
            "en": "Yeah. I—don’t know. Maybe a distortion—thing or—yeah. Strange, to say the least.",
            "ru_native": "Да. Даже не знаю. Может, просто искажение какое. Странно, мягко говоря.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0006",
            "en": "Hey! Do you see that?",
            "ru_native": "",
            "ru_machine": "DEEPL Эй! Ты видишь это?",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0007",
            "en": "Those lights.",
            "ru_native": "",
            "ru_machine": "DEEPL Эти огни.",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0008",
            "en": "Who is that?",
            "ru_native": "",
            "ru_machine": "DEEPL Кто это?",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0009",
            "en": "C’mon, let’s catch up to them! We might only have one chance at this.",
            "ru_native": "",
            "ru_machine": "DEEPL Давай, догоним их! Возможно, у нас есть только один шанс на это.",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0010",
            "en": "They could know what’s going on!",
            "ru_native": "",
            "ru_machine": "DEEPL Они могут знать, что происходит!",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0011",
            "en": "Wait up! Guys!",
            "ru_native": "",
            "ru_machine": "DEEPL Подожди! Ребята!",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_JACOB_0012",
            "en": "Let’s, uh, just get back to shore. [sigh] So bizarre.",
            "ru_native": "Давай возвращаться на берег. Безумие какое-то.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0000",
            "en": "They're going to give us, everything we want.",
            "ru_native": "...все, что захотим.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0001",
            "en": "No, everything we want—it’s not—you’ll get out, too, Violet.",
            "ru_native": "Нет, все, что захотим! Ты... ты тоже вырвешься, Вайолет.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0002",
            "en": "No, he’s right, Violet.",
            "ru_native": "Нет, Вайолет, он прав.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0003",
            "en": "We can’t back away as soon as stuff starts working.",
            "ru_native": "У нас только начало что-то получаться.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0004",
            "en": "This was the whole thing, remember?",
            "ru_native": "Мы этого и хотели, разве нет?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0005",
            "en": "So it’s a little rainy. So what?",
            "ru_native": "Да, сегодня накрапывает. Ну и что?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0006",
            "en": "It’s like they said, April showers.",
            "ru_native": "Апрельские дожди, все дела.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0007",
            "en": "If you’re scared, go home.",
            "ru_native": "Если боишься, вали домой.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0008",
            "en": "But don’t come back if you do, Violet.",
            "ru_native": "И можешь не возвращаться, Вайолет.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0009",
            "en": "There won’t be anyone waiting.",
            "ru_native": "Ждать тебя никто не будет.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0010",
            "en": "Let’s find a transmitter, and then we can have this discussion again. If we must.",
            "ru_native": "Найдем передатчик, и потом вернемся к этому разговору. Если придется.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0011",
            "en": "Just—crap. Head to the transmitter.",
            "ru_native": "Достала. Иди к передатчику.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_OLIVIA_0012",
            "en": "Hang up! Shut up and hang up.",
            "ru_native": "Отключись! Закрой рот и отключись.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0000",
            "en": "Hey, whoever this is, what did you guys do? What’s going on?",
            "ru_native": "Эй, послушайте, что вы сделали? Что происходит?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0001",
            "en": "Hey! Hello—hello? Who is this, who’s talking?",
            "ru_native": "Эй! Прием!.. Прием? Кто это, кто говорит?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0002",
            "en": "Yeah, nice to meet you, too.",
            "ru_native": "Да, приятно познакомиться.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0003",
            "en": "How’d they know I was listening in?",
            "ru_native": "Как они узнали, что я их слушаю?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0004",
            "en": "That didn’t sound—ya know—like a person.",
            "ru_native": "Не похоже на... человека.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0005",
            "en": "Yeah, yeah, I do. Finally, somebody else.",
            "ru_native": "",
            "ru_machine": "DEEPL Да-да, именно так. Наконец-то, кто-то еще.",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0006",
            "en": "Uh, those kids we just heard, maybe?",
            "ru_native": "",
            "ru_machine": "DEEPL А, может, те дети, которых мы только что слышали?",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0007",
            "en": "Wait! Jacob, hold up!",
            "ru_native": "",
            "ru_machine": "DEEPL Подожди! Джейкоб, подожди!",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0008",
            "en": "Wait, Jacob, we don’t know them!",
            "ru_native": "",
            "ru_machine": "DEEPL Подожди, Джейкоб, мы их не знаем!",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_RILEY_0009",
            "en": "Wait! Jacob, don’t run off! Let’s stick together!",
            "ru_native": "",
            "ru_machine": "DEEPL Подожди! Джейкоб, не убегай! Давай держаться вместе!",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VIOLET_0000",
            "en": "I don’t know, Olivia—",
            "ru_native": "Оливия, я не знаю...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VIOLET_0001",
            "en": "What? Seriously?",
            "ru_native": "Что? Ты серьезно?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VIOLET_0002",
            "en": "There’s a friggin’ purple and green hellscape in the sky because of us!",
            "ru_native": "Мы вызвали в небе гребаное окно в фиолетово-зеленый ад!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VIOLET_0003",
            "en": "I just—",
            "ru_native": "Просто...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VIOLET_0004",
            "en": "Wait, but—",
            "ru_native": "Стой, но...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VOICE_0000",
            "en": "One, two, three, four, five. Once I caught a fish alive,",
            "ru_native": "Раз, два, три, четыре, пять. Рыбку удалось поймать.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VOICE_0001",
            "en": "Six, seven, eight, nine, ten. Then I let it go again.",
            "ru_native": "Шесть, семь, восемь, девять, десять. Пусть плывет, не буду есть.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VOICE_0002",
            "en": "Why did you let it go? Because it bit my finger so.",
            "ru_native": "Рыбка жалость заслужила? Нет, за палец укусила.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VOICE_0003",
            "en": "Which finger did it bite? This little finger on the right.",
            "ru_native": "Что же это за гостинец? Правая рука – мизинец.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        },
        {
            "tag": "A1LH.WALTAL_VOICE_0004",
            "en": "Mind your manners, little boy and girl.",
            "ru_native": "Следите за манерами, ребятки.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": ""
        }
    ]
}