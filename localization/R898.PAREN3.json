{
    "bundle": "loc_packages_assets_",
    "scene": "R898.PAREN3",
    "entries": [
        {
            "tag": "R898.PAREN3_ZWICKY_0000",
            "en": "In a world without light what is there to fear?",
            "ru_native": "Чего бояться в мире без света?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "У світі без світла — чого боятися?"
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0001",
            "en": "The unknown?",
            "ru_native": "Неизвестного?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Невідомого?"
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0002",
            "en": "In darkness, we are all unknown.",
            "ru_native": "Во тьме все мы неизвестны.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "У темряві ми всі невідомі."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0003",
            "en": "Do not fear the dark.",
            "ru_native": "Не бойся тьмы.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Не бійся темряви."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0004",
            "en": "It doesn’t fear you.",
            "ru_native": "Она не боится тебя.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вона не боїться тебе."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0005",
            "en": "Instead, disciples of Parentage, fear the light.",
            "ru_native": "Нет, ученики «Наследия», бойтесь света.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Натомість, послідовники «Парентажу», бійтеся світла."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0006",
            "en": "Its judgment. Stark. Sobering. Irrefutable.",
            "ru_native": "Его осуждения. Грубого. Трезвого. Непреклонного.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Його засудження. Повного. Ридання. Неспростовного."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0007",
            "en": "For the rose: light is life.",
            "ru_native": "Для розы свет есть жизнь.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Для троянди світло — це життя."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0008",
            "en": "For the dewdrop clinging to the rose’s petals: light is death.",
            "ru_native": "Для капли росы на лепестке розы свет есть смерть.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Для краплі роси на пелюстці троянди світло — смерть."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0009",
            "en": "Light takes the drop to pieces.",
            "ru_native": "Свет разрывает росинку на части.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Світло знищує краплю."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0010",
            "en": "Slow death. Unrelenting death. Indifferent death.",
            "ru_native": "Медленная смерть. Неумолимая смерть. Безжалостная смерть.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Повільна смерть. Невблаганна. Байдужа смерть."
        },
        {
            "tag": "R898.PAREN3_ZWICKY_0011",
            "en": "Disciples...fear the light.",
            "ru_native": "Ученики... бойтесь света.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Послідовники, бійтеся світла."
        }
    ]
}