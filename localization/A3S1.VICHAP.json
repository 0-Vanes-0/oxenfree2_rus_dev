        {
            "tag": "A3S1.VICHAP_JACOB_0014",
            "en": "Kids.",
            "ru_native": "Дети.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Діти."
        },
        {
            "tag": "A3S1.VICHAP_JACOB_0015",
            "en": "Right?",
            "ru_native": "Да?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Адже так?"
