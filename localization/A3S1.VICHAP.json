            "tag": "A3S1.VICHAP_RILEY_0009",
            "en": "You guys friendly?",
            "ru_native": "Вы дружите?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ви приятелюєте?"
