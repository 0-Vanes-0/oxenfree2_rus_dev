{
    "bundle": "loc_packages_assets_",
    "scene": "A3S6.THSTOR",
    "entries": [
        {
            "tag": "A3S6.THSTOR_ALEX_0000",
            "en": "Riley, what are you doing? Tune into the portal!",
            "ru_native": "Райли, что ты делаешь? Настройся на портал!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі, що ти робиш? Налаштуйся на портал!"
        },
        {
            "tag": "A3S6.THSTOR_ALEX_0001",
            "en": "Why do you think we ran around to see everyone?",
            "ru_native": "А почему мы носимся, чтобы всех увидеть?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Чому ми бігаємо, щоби побачити всіх?"
        },
        {
            "tag": "A3S6.THSTOR_ALEX_0002",
            "en": "They’re ready! Let’s go!",
            "ru_native": "Они готовы, погнали!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Вони готові! До справи!"
        },
        {
            "tag": "A3S6.THSTOR_ALEX_0003",
            "en": "Do it!",
            "ru_native": "Давай!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Давай!"
        },
        {
            "tag": "A3S6.THSTOR_ALEX_0004",
            "en": "Yes!",
            "ru_native": "Да!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Так!"
        },
        {
            "tag": "A3S6.THSTOR_ALEX_0005",
            "en": "I don’t have time to walk you through the science, okay—",
            "ru_native": "У меня нет времени на научные объяснения.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "У мене немає часу пояснювати теорію, ясно?"
        },
        {
            "tag": "A3S6.THSTOR_ALEX_0006",
            "en": "—just we need to boost the frequency so everyone’s signal will get a lift.",
            "ru_native": "Нам надо прост осилить частоту, чтобы поднять всем сигнал.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Нам потрібно лише посилити частоту, щоби підняти всім сигнал."
        },
        {
            "tag": "A3S6.THSTOR_ALEX_0007",
            "en": "Seriously, Riley, it’s now or never.",
            "ru_native": "Серьезно, Райли, сейчас или никогда.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Серйозно, Райлі, тепер або ніколи."
        },
        {
            "tag": "A3S6.THSTOR_ALEX_0008",
            "en": "Maggie, Clarissa and Rex will do their part but you need to do yours.",
            "ru_native": "Мэгги, Кларисса и Рекс сделают свою часть, но ты должна сделать свою.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Меггі, Клариса й Рекс зроблять свою частину, але ми маємо зробити свою."
        },
        {
            "tag": "A3S6.THSTOR_CHOICE_0000",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A3S6.THSTOR_CHOICE_0000_OPTION_0000",
            "en": "Oh!",
            "ru_native": "Оу!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "О!"
        },
        {
            "tag": "A3S6.THSTOR_CHOICE_0000_OPTION_0001",
            "en": "Really?",
            "ru_native": "Правда?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Справді?"
        },
        {
            "tag": "A3S6.THSTOR_CHOICE_0000_OPTION_0002",
            "en": "And that will do what?!",
            "ru_native": "И что тогда будет?!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "І що буде тоді?!"
        },
        {
            "tag": "A3S6.THSTOR_JACOB_0000",
            "en": "Whoa! God, this—I don’t know, Riley—this doesn’t—",
            "ru_native": "Ух ты! Боже, это... Не знаю, Райли... Это не...",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Ого! Господи… я не знаю… Райлі, це не…"
        },
        {
            "tag": "A3S6.THSTOR_JACOB_0001",
            "en": "Huh? What do you mean?",
            "ru_native": "А? В смысле?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Га? Що це значить?"
        },
        {
            "tag": "A3S6.THSTOR_RILEY_0000",
            "en": "Oh! Oh, okay.",
            "ru_native": "О! Ладно.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "О! О, гаразд."
        },
        {
            "tag": "A3S6.THSTOR_RILEY_0001",
            "en": "Wait, really?",
            "ru_native": "Стой, правда?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Що, справді?"
        },
        {
            "tag": "A3S6.THSTOR_RILEY_0002",
            "en": "Wait, what? What will that do?",
            "ru_native": "Постой, что? Как это?",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Чекай, що? Який у цьому сенс?"
        }
    ]
}