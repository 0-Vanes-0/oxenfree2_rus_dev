    "entries": [
        {
            "tag": "A2C3.RILJUM_CHOICE_0000",
            "en": "DO NOT DELETE OR CHANGE",
            "ru_native": "DO NOT DELETE OR CHANGE",
            "ru_machine": "",
            "ru_final": "DO NOT DELETE OR CHANGE",
            "verified": true,
            "uk": "DO NOT DELETE OR CHANGE"
        },
        {
            "tag": "A2C3.RILJUM_CHOICE_0000_OPTION_0000",
            "en": "Nothing broken.",
            "ru_native": "Кости целы.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Кістки на місці."
        },
        {
            "tag": "A2C3.RILJUM_CHOICE_0000_OPTION_0001",
            "en": "I’m dead.",
            "ru_native": "Я умерла.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я померла."
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0000",
            "en": "Riley!",
            "ru_native": "Райли!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі!"
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0001",
            "en": "Riley!",
            "ru_native": "Райли!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі!"
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0002",
            "en": "Oh, Jesus. Are you okay?!",
            "ru_native": "Господи. Ты живая?!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Господи, ти жива?!"
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0003",
            "en": "Please say something!",
            "ru_native": "Скажи что-нибудь!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Скажи хоч що-небудь, благаю!"
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0004",
            "en": "Riley!",
            "ru_native": "Райли!",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Райлі!"
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0005",
            "en": "Okay, I think—are you moving? You’re moving. That’s good.",
            "ru_native": "Так, вроде... Ты двигаешься? Двигаешься. Уже хорошо.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Зрозуміло… ти можеш рухатися? Ти рухаєшся. Це чудово."
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0006",
            "en": "I’ll find a way down just—hang on.",
            "ru_native": "Я сейчас спущусь, подожди.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Зараз я до тебе спущуся… почекай."
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0007",
            "en": "Gimme—gimme a second.",
            "ru_native": "Одну... секундочку.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Секун… секундочку."
        },
        {
            "tag": "A2C3.RILJUM_JACOB_0008",
            "en": "I think we can safely say that it’s just the way at this point.",
            "ru_native": "Думаю, мы оба понимаем, что сейчас это единственный вариант.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Гадаю, можна впевнено сказати, що наразі це єдиний спосіб."
        },
        {
            "tag": "A2C3.RILJUM_RILEY_0000",
            "en": "I’m—okay. Nothing broken. I don’t think.",
            "ru_native": "Я жива. Кости целы. Кажется.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я… жива. Нічого не зламала. Думаю, що ні."
        },
        {
            "tag": "A2C3.RILJUM_RILEY_0001",
            "en": "I’m dead. Or maybe I just wish I was dead.",
            "ru_native": "Я умерла. Или просто хочу умереть.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "Я померла. Або ж мені б так хотілося."
        },
        {
            "tag": "A2C3.RILJUM_RILEY_0002",
            "en": "Ah, so… taking the easy way down, I see.",
            "ru_native": "Ага, так... просто иди дальше.",
            "ru_machine": "",
            "ru_final": "",
            "verified": false,
            "uk": "А-а, зрозуміло... обрав найлегший спосіб спуститися."
        }
    ]
}